{
    "name": "bitpatty/discord_pb_tracker",
    "type": "project",
    "description": "A speedrun.com PB tracker for use with discord.",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "doctrine/dbal": "2.10.3",
        "fideloper/proxy": "4.4.0",
        "guzzlehttp/guzzle": "7.0.1",
        "laravel/framework": "6.18.40",
        "laravel/tinker": "2.4.2",
        "socialiteproviders/discord": "2.0.2"
    },
    "require-dev": {
        "facade/ignition": "1.16.3",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "3.0.1",
        "phpunit/phpunit": "9.3.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
